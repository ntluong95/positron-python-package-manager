{
  "name": "positron-python-package-manager",
  "displayName": "%display.name%",
  "description": "%description%",
  "version": "0.1.3",
  "publisher": "ntluong95",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntluong95/positron-python-package-manager.git"
  },
  "icon": "resources/icon1.png",
  "license": "MIT",
  "extensionKind": [
    "workspace"
  ],
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages",
    "Data Science",
    "Visualization"
  ],
  "keywords": [
    "python",
    "package",
    "Positron",
    "pandas",
    "install",
    "uninstall",
    "update",
    "UV",
    "dependencies",
    "PyPI",
    "pip",
    "package manager",
    "search"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:pip-requirements",
    "onLanguage:toml",
    "onLanguage:plaintext"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "%short.name%",
      "properties": {
        "positron-python-package-manager.filterOutdatedIfUpToDateElsewhere": {
          "type": "boolean",
          "scope": "machine-overridable",
          "default": true,
          "description": "%filterOutdatedIfUpToDateElsewhere%"
        },
        "pypiAssistant.codeLens": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable latest package version CodeLens in pip-requirements and pyproject.toml files."
        },
        "positronPythonPackageManager.enableVersionDecorations": {
          "type": "boolean",
          "default": false,
          "description": "Enable decorations showing if package versions are up-to-date or outdated in pip-requirements and pyproject.toml files."
        },
        "inlinePythonPackageInstaller.autoInstall": {
          "type": "boolean",
          "default": false,
          "description": "Automatically install missing Python modules without prompting."
        },
        "inlinePythonPackageInstaller.customPipCommand": {
          "type": "string",
          "default": "pip install",
          "description": "Custom pip command to use for installing modules."
        }
      }
    },
    "commands": [
      {
        "command": "extension.installModule",
        "title": "PPM: Install Missing Python Module",
        "icon": "$(desktop-download)"
      },
      {
        "command": "positron-python-package-manager.installPackages",
        "title": "%install.packages%",
        "icon": "$(desktop-download)"
      },
      {
        "command": "positron-python-package-manager.uninstallPackage",
        "title": "%uninstall.package%",
        "icon": "$(trash)"
      },
      {
        "command": "positron-python-package-manager.checkOutdatedPackages",
        "title": "PPM: Check Outdated Packages",
        "icon": "$(arrow-circle-up)"
      },
      {
        "command": "positron-python-package-manager.searchPackages",
        "title": "%search.packages%",
        "icon": "$(search)"
      },
      {
        "command": "positron-python-package-manager.refreshPackages",
        "title": "%refresh.packages%",
        "icon": "$(refresh)"
      },
      {
        "command": "uv-wingman.buildEnvironment",
        "title": "PPM: Build Environment from opened file",
        "category": "UV"
      },
      {
        "command": "uv-wingman.buildEnvironment.ui",
        "title": "Build Environment",
        "icon": "$(tools)",
        "actionBarOptions": {
          "controlType": "button",
          "displayTitle": true
        }
      },
      {
        "command": "uv-wingman.installPackagesUV",
        "title": "PPM: install packages from opened file",
        "category": "UV"
      },
      {
        "command": "uv-wingman.installPackagesUV.ui",
        "title": "Install Packages",
        "icon": "$(symbol-event)",
        "actionBarOptions": {
          "controlType": "button",
          "displayTitle": true
        }
      },
      {
        "command": "uv-wingman.writeRequirementsFile",
        "title": "PPM: Write a requirements.txt file from the active environment",
        "category": "UV"
      },
      {
        "command": "uv-wingman.writeRequirementsFile.ui",
        "title": "Write Requirement File",
        "icon": "$(book)",
        "actionBarOptions": {
          "controlType": "button",
          "displayTitle": true
        }
      },
      {
        "command": "uv-wingman.deleteEnvironment",
        "title": "PPM: Delete Environment",
        "category": "UV"
      },
      {
        "command": "uv-wingman.deleteEnvironment.ui",
        "title": "Delete Environment",
        "icon": "$(trashcan)",
        "actionBarOptions": {
          "controlType": "button",
          "displayTitle": true
        }
      },
      {
        "command": "uv.init",
        "title": "PPM: Initialize Project",
        "category": "UV"
      },
      {
        "command": "uv.sync",
        "title": "PPM: Sync Dependencies",
        "category": "UV"
      },
      {
        "command": "uv.upgrade",
        "title": "PPM: Upgrade Packages",
        "category": "UV"
      },
      {
        "command": "uv.cache.clean",
        "title": "PPM: Clean Cache",
        "category": "UV"
      },
      {
        "command": "uv.removePackage",
        "title": "PPM: Remove Package from pyproject.toml",
        "category": "UV"
      },
      {
        "command": "uv.searchPackage",
        "title": "PPM: Search Package on PyPI",
        "category": "UV"
      },
      {
        "command": "uv.generateLock",
        "title": "PPM: Generate Lock File",
        "category": "UV"
      },
      {
        "command": "uv.upgradeDependencies",
        "title": "PPM: Upgrade Dependencies to the latest",
        "category": "UV"
      },
      {
        "command": "uv.manageVirtualEnv",
        "title": "PPM: Create Virtual Environment",
        "category": "UV"
      },
      {
        "command": "uv.runScript",
        "title": "PPM: Run Python Script",
        "category": "UV"
      },
      {
        "command": "uv.addScriptDependency",
        "title": "PPM: Add Script Dependency",
        "category": "UV"
      },
      {
        "command": "uv.installPython",
        "title": "PPM: Install Python Version",
        "category": "UV"
      },
      {
        "command": "uv.pinPython",
        "title": "PPM: Pin Python Version",
        "category": "UV"
      },
      {
        "command": "uv.installTool",
        "title": "PPM: Install Tool",
        "category": "UV"
      },
      {
        "command": "uv.runTool",
        "title": "PPM: Run Tool with UVX",
        "category": "UV"
      },
      {
        "command": "uv.add",
        "title": "PPM: Add Package to Project",
        "category": "UV"
      },
      {
        "command": "uv.init",
        "title": "PPM: Initialize Project",
        "category": "UV"
      },
      {
        "command": "uv.sync",
        "title": "PPM: Sync Dependencies",
        "category": "UV"
      },
      {
        "command": "uv.upgrade",
        "title": "PPM: Upgrade Packages",
        "category": "UV"
      },
      {
        "command": "uv.cache.clean",
        "title": "PPM: Clean Cache",
        "category": "UV"
      },
      {
        "command": "uv.removePackage",
        "title": "PPM: Remove Package from pyproject.toml",
        "category": "UV"
      },
      {
        "command": "uv.searchPackage",
        "title": "PPM: Search Package on PyPI",
        "category": "UV"
      },
      {
        "command": "uv.generateLock",
        "title": "PPM: Generate Lock File",
        "category": "UV"
      },
      {
        "command": "uv.upgradeDependencies",
        "title": "PPM: Upgrade Dependencies to the latest",
        "category": "UV"
      },
      {
        "command": "uv.manageVirtualEnv",
        "title": "PPM: Create Virtual Environment",
        "category": "UV"
      },
      {
        "command": "uv.runScript",
        "title": "PPM: Run Python Script",
        "category": "UV"
      },
      {
        "command": "uv.addScriptDependency",
        "title": "PPM: Add Script Dependency",
        "category": "UV"
      },
      {
        "command": "uv.installPython",
        "title": "PPM: Install Python Version",
        "category": "UV"
      },
      {
        "command": "uv.pinPython",
        "title": "PPM: Pin Python Version",
        "category": "UV"
      },
      {
        "command": "uv.installTool",
        "title": "PPM: Install Tool",
        "category": "UV"
      },
      {
        "command": "uv.runTool",
        "title": "PPM: Run Tool with UVX",
        "category": "UV"
      },
      {
        "command": "uv.add",
        "title": "PPM: Add Package to Project",
        "category": "UV"
      },
      {
        "command": "positron-python-package-manager.toggleVersionDecorations",
        "title": "Package version status",
        "actionBarOptions": {
          "controlType": "toggle",
          "leftTitle": "Hide",
          "rightTitle": "Show",
          "toggled": "config.positronPythonPackageManager.enableVersionDecorations"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "positron-python-packages",
          "title": "%packages%",
          "icon": "resources/dep.svg"
        }
      ]
    },
    "views": {
      "positron-python-packages": [
        {
          "id": "pythonPackageView",
          "name": "Python",
          "icon": "resources/dep.svg"
        }
      ]
    },
    "menus": {
      "editor/actions/right": [
        {
          "command": "positron-python-package-manager.toggleVersionDecorations",
          "when": "resourceFilename == requirements.txt || resourceFilename == pyproject.toml",
          "group": "1_editor_mode"
        }
      ],
      "editor/actions/left": [
        {
          "command": "uv-wingman.buildEnvironment.ui",
          "group": "uv@1",
          "when": "resourceFilename == requirements.txt || resourceFilename == pyproject.toml"
        },
        {
          "command": "uv-wingman.installPackagesUV.ui",
          "group": "uv@2",
          "when": "resourceFilename == requirements.txt"
        },
        {
          "command": "uv-wingman.writeRequirementsFile.ui",
          "group": "uv@3",
          "when": "resourceFilename == requirements.txt || resourceFilename == pyproject.toml"
        },
        {
          "command": "uv-wingman.deleteEnvironment.ui",
          "group": "uv@4",
          "when": "resourceFilename == requirements.txt || resourceFilename == pyproject.toml"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.add",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.removePackage",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.searchPackage",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.generateLock",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.upgradeDependencies",
          "group": "uv"
        }
      ],
      "editor/context": [
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.add",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.removePackage",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.searchPackage",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.generateLock",
          "group": "uv"
        },
        {
          "when": "resourceFilename == pyproject.toml",
          "command": "uv.upgradeDependencies",
          "group": "uv"
        }
      ],
      "view/title": [
        {
          "command": "positron-python-package-manager.installPackages",
          "when": "view == pythonPackageView",
          "group": "navigation@1"
        },
        null,
        {
          "command": "positron-python-package-manager.checkOutdatedPackages",
          "when": "view == pythonPackageView",
          "group": "navigation@2"
        },
        {
          "command": "positron-python-package-manager.searchPackages",
          "when": "view == pythonPackageView",
          "group": "navigation@99"
        },
        {
          "command": "positron-python-package-manager.filterLoadedPackages",
          "when": "view == pythonPackageView",
          "group": "navigation@3"
        },
        {
          "command": "positron-python-package-manager.refreshPackages",
          "when": "view == pythonPackageView",
          "group": "navigation@100"
        }
      ],
      "view/item/context": [
        {
          "command": "positron-python-package-manager.uninstallPackage",
          "when": "view == pythonPackageView && viewItem == PyPackage",
          "group": "inline@999"
        },
        {
          "command": "positron-python-package-manager.updatePackage",
          "when": "view == pythonPackageView && viewItem == canUpdate",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/fuzzaldrin-plus": "^0.6.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/semver": "^7.7.0",
    "@types/vscode": "1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^29.0.3",
    "lint-staged": "^13.2.1",
    "mocha": "^10.8.2",
    "prettier": "^2.5.1",
    "rimraf": "^4.0.0",
    "sinon": "^19.0.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "fuzzaldrin-plus": "^0.6.0",
    "lru-cache": "^10.2.0",
    "node-fetch": "^3.3.2",
    "pip-requirements-js": "^0.2.1",
    "semver": "^7.7.1",
    "toml": "^3.0.0",
    "toml-eslint-parser": "^0.10.0",
    "wretch": "^2.11.0"
  },
  "l10n": "./l10n"
}
